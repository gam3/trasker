
bindir=@bindir@
exec_prefix = @exec_prefix@
prefix = @prefix@

all : tlist

tlist : qmakefile
	make -f qmakefile

qmakefile : tlist.pro Makefile
	qmake "CONFIG+=release" "MAKEFILE=qmakefile" "target.path = $(bindir)"

clean :
	make -f qmakefile clean

distdir:
	echo distdir

distclean: clean
	rm -f Makefile


.PRECIOUS: Makefile
Makefile: Makefile.in
	sh config.status

install:
	make INSTALL_ROOT=${DESTDIR} -f qmakefile install

install-info:

install-html:

install-pdf:



